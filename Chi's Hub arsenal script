                        EspSection:addToggle("Chams", nil ,function(state)
                            getgenv().cham = state
                            local text = ""
        
                            if getgenv().cham then
                                text = "On"
                            else
                                text = "Off"
                            end
                            local color = BrickColor.new(0, 0 ,255)
                            local transparency = .8
                            
                            local Players = game:GetService("Players")
                            local function _ESP(c)
                                repeat wait() until c.PrimaryPart ~= nil
                                for i,p in pairs(c:GetChildren()) do
                                    if p.ClassName == "Part" or p.ClassName == "MeshPart" then
                                        if p:FindFirstChild("Chams") then p.Chams:Destroy() end
                                        local a = Instance.new("BoxHandleAdornment",p)
                                        a.Name = "Chams"
                                        a.Size = p.Size
                                        a.Color = color
                                        a.Transparency = transparency
                                        a.AlwaysOnTop = true    
                                        a.Visible = true    
                                        a.Adornee = p
                                        a.ZIndex = true    
                            
                                    end
                                end
                            end
                            local function ESP()
                                for i,v in pairs(Players:GetChildren()) do
                                    if v ~= game.Players.LocalPlayer and v.TeamColor ~= Players.LocalPlayer.TeamColor then
                                        if v.Character then
                                            _ESP(v.Character)
                                        end
                                        v.CharacterAdded:Connect(function(chr)
                                            _ESP(chr)
                                        end)
                                    end
                                end
                                Players.PlayerAdded:Connect(function(player)
                                    player.CharacterAdded:Connect(function(chr)
                                        _ESP(chr)
                                    end)  
                                end)
                            end
                            ESP()
        
                            local function clearEsp(c)
                                repeat wait() until c.PrimaryPart ~= nil
                                for i,p in pairs(c:GetChildren()) do
                                    if p.ClassName == "Part" or p.ClassName == "MeshPart" then
                                        if p:FindFirstChild("Chams") then p.Chams:Destroy() end
                                    end
                                end
                            end
        
                            if getgenv().cham then 
                                venyx:Notify("Info:", text)
                            else
                                venyx:Notify("Info:", text)
                                for i, v in pairs(Players:GetPlayers()) do
                                    if v.Character then
                                        clearEsp(v.Character)
                                    end
                                end
                            end

                            while wait() do
                                    if autoUpdate then
                                    for i, v in pairs(Players:GetPlayers()) do
                                        if v.Character then
                                            clearEsp(v.Character)
                                        end
                                    end
                                    wait(1)
                                    ESP()
                                    wait(updateTime)
                                end
                            end
        
                        end)
        
                        EspSection:addToggle("Tracers", nil, function(value)
                            getgenv().tracer = value
                            local text = ""
        
                            if getgenv().tracer then
                                text = "On"
                            else
                                text = "Off"
                            end
                            local lplr = game.Players.LocalPlayer
                            local camera = game:GetService("Workspace").CurrentCamera
                            local CurrentCamera = workspace.CurrentCamera
                            local worldToViewportPoint = CurrentCamera.worldToViewportPoint
                    
                            for i,v in pairs(game.Players:GetChildren()) do
                                local Tracer = Drawing.new("Line")
                                Tracer.Visible = false
                                if useDefaultTeamColors then
                                    Tracer.Color = v.TeamColor.Color
                                else
                                    Tracer.Color = tracersColor
                                end
                                Tracer.Thickness = 1
                                Tracer.Transparency = 1
                    
                                function lineesp()
                                    game:GetService("RunService").RenderStepped:Connect(function()
                                        if v.Character ~= nil and v.Character:FindFirstChild("Humanoid") ~= nil and v.Character:FindFirstChild("HumanoidRootPart") ~= nil and v ~= lplr and v.Character.Humanoid.Health > 0 then
                                            local Vector, OnScreen = camera:worldToViewportPoint(v.Character.HumanoidRootPart.Position)
                    
                                            if OnScreen then
                                                Tracer.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 2)
                                                Tracer.To = Vector2.new(Vector.X, Vector.Y)
                    
                                                if _G.TeamCheck and v.TeamColor == lplr.TeamColor then
                                                    --//Teammates
                                                    Tracer.Visible = false
                                                else
                                                    --//Enemies
                                                    Tracer.Visible = true
                                                end
                                            else
                                                Tracer.Visible = false
                                            end
                                        else
                                            Tracer.Visible = false
                                        end
                                    end)
                                end
                                coroutine.wrap(lineesp)()
                            end
                    
                            game.Players.PlayerAdded:Connect(function(v)
                                local Tracer = Drawing.new("Line")
                                Tracer.Visible = false
                                if useDefaultTeamColors then
                                    Tracer.Color = v.TeamColor.Color
                                else
                                    Tracer.Color = tracersColor
                                end
                                Tracer.Thickness = 1
                                Tracer.Transparency = 1
                    
                                function lineesp()
                                    game:GetService("RunService").RenderStepped:Connect(function()
                                        if v.Character ~= nil and v.Character:FindFirstChild("Humanoid") ~= nil and v.Character:FindFirstChild("HumanoidRootPart") ~= nil and v ~= lplr and v.Character.Humanoid.Health > 0 then
                                            local Vector, OnScreen = camera:worldToViewportPoint(v.Character.HumanoidRootPart.Position)
                    
                                            if OnScreen then
                                                Tracer.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 2)
                                                Tracer.To = Vector2.new(Vector.X, Vector.Y)
                    
                                                if _G.TeamCheck and v.TeamColor == lplr.TeamColor then
                                                    --//Teammates
                                                    Tracer.Visible = false
                                                else
                                                    --//Enemies
                                                    Tracer.Visible = true
                                                end
                                            else
                                                Tracer.Visible = false
                                            end
                                        else
                                            Tracer.Visible = false
                                        end
                                    end)
                                end
                                coroutine.wrap(lineesp)()
                            end)
        
                            if getgenv().tracer then
                                venyx:Notify("Info:", text)
                            else
                                venyx:Notify("Info:", text)
                            end
                        end)
